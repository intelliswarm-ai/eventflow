          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  COUNT(*) as order_count,\n  order_count_bucket\nFROM (\n  SELECT \n    user_id,\n    COUNT(*) as user_orders,\n    CASE \n      WHEN COUNT(*) = 1 THEN '1 order'\n      WHEN COUNT(*) BETWEEN 2 AND 5 THEN '2-5 orders'\n      WHEN COUNT(*) BETWEEN 6 AND 10 THEN '6-10 orders'\n      ELSE '10+ orders'\n    END as order_count_bucket\n  FROM orders_raw\n  GROUP BY user_id\n) user_segments\nGROUP BY order_count_bucket\nORDER BY \n  CASE order_count_bucket\n    WHEN '1 order' THEN 1\n    WHEN '2-5 orders' THEN 2\n    WHEN '6-10 orders' THEN 3\n    ELSE 4\n  END;",
          "refId": "A"
        }
      ],
